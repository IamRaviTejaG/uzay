{
  "name": "uzay",
  "version": "0.0.1",
  "description": "Uzay is a Node.js and MongoDB based modern blog engine.",
  "main": "index.js",
  "scripts": {
    "docs": "./node_modules/.bin/apidoc -i ./routes -o ./docs",
    "precommit": "npm test",
    "postinstall": "node config/copyConfig.js",
    "start": "./node_modules/.bin/nodemon index.js",
    "test": "./node_modules/.bin/standard && ./node_modules/.bin/mocha"
  },
  "author": "Saksham Saxena",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakshamsaxena/uzay.git"
  },
  "apidoc": {
    "title": "Uzay",
    "order": [
      "GetBlogPostByID",
      "GetBlogPostsByTagName",
      "GetCommentByID",
      "PostCommentToBlogPost",
      "PatchLikeOnBlogPost",
      "PatchDislikeOnBlogPost",
      "PatchLikeOnBlogPostComment",
      "PatchDislikeOnBlogPostComment"
    ]
  },
  "standard": {
    "ignore": [
      "docs"
    ]
  },
  "dependencies": {
    "bcrypt": "3.0.0",
    "body-parser": "1.18.3",
    "express": "4.16.3",
    "jsonwebtoken": "8.3.0",
    "mongoose": "5.2.7",
    "mongoose-plugin-autoinc": "1.1.9",
    "morgan": "1.9.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "nodemon": "1.18.3",
    "should": "13.1.3",
    "standard": "11.0.0",
    "supertest": "3.0.0"
  }
}
